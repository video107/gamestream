<div class="container">
  <div class="row">
  <h1>專屬連結列表</h1>
  <% if current_user.fb_image %>
        <%=image_tag(current_user.fb_image,:class => "img-circle") %>
      <% else %>
        <h2><%= current_user.user_name %></h2>
  <% end %>

   <% @cases.each do |c| %>
    <ul>
      <li style="list-style-type:none">遊戲名稱：<%= c.menu.name %></li>
      <li style="list-style-type:none">專屬連結：<%= link_to "localhost:3000"+c.case_url, c.case_url %></li>
      <!-- <li style="list-style-type:none">專屬連結：<%#= link_to "139.162.19.7"+c.case_url, c.case_url %></li> -->
    </ul>
   <% end %>
   <%= link_to "回到遊戲頁面",root_path,:class => "btn btn-default" %>
 </div>
</div>