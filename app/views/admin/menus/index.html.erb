<p id="notice"><%= notice %></p>

<h1>Listing Admin Menus</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Name</th>
      <th>Contents</th>
      <th>Menu url</th>
      <th>Menu url ios</th>
      <th>Game pic</th>
      <th>Game pic 2</th>
      <th>Game pic 3</th>
      <th>Game pic 4</th>
      <th>Game pic 5</th>
      <th>Game icon</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_menus.each do |admin_menu| %>
      <tr>
        <td><%= admin_menu.image %></td>
        <td><%= admin_menu.name %></td>
        <td><%= admin_menu.contents %></td>
        <td><%= admin_menu.menu_url %></td>
        <td><%= admin_menu.menu_url_ios %></td>
        <td><%= admin_menu.game_pic %></td>
        <td><%= admin_menu.game_pic_2 %></td>
        <td><%= admin_menu.game_pic_3 %></td>
        <td><%= admin_menu.game_pic_4 %></td>
        <td><%= admin_menu.game_pic_5 %></td>
        <td><%= admin_menu.game_icon %></td>
        <td><%= link_to 'Show', admin_menu %></td>
        <td><%= link_to 'Edit', edit_admin_menu_path(admin_menu) %></td>
        <td><%= link_to 'Destroy', admin_menu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Menu', new_admin_menu_path %>
